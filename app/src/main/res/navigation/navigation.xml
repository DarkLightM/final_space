<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/start">
    <fragment
        android:id="@+id/start"
        android:name="com.example.androidcoursework.fragments.Start"
        android:label="Final Space Encyclopedia"
        tools:layout="@layout/fragment_start" >
        <action
            android:id="@+id/action_start3_to_characterFragment2"
            app:destination="@id/characterFragment" />
        <action
            android:id="@+id/action_start3_to_episodesFragment"
            app:destination="@id/episodesFragment" />
        <action
            android:id="@+id/action_start3_to_quotesFragment"
            app:destination="@id/quotesFragment" />
        <action
            android:id="@+id/action_start3_to_locationsFragment"
            app:destination="@id/locationsFragment" />
    </fragment>
    <fragment
        android:id="@+id/characterFragment"
        android:name="com.example.androidcoursework.fragments.character.CharactersFragment"
        android:label="Characters"
        tools:layout="@layout/fragment_character" >
    </fragment>
    <fragment
        android:id="@+id/episodesFragment"
        android:name="com.example.androidcoursework.fragments.episode.EpisodesFragment"
        android:label="Episodes"
        tools:layout="@layout/fragment_episodes" />
    <fragment
        android:id="@+id/locationsFragment"
        android:name="com.example.androidcoursework.fragments.location.LocationsFragment"
        android:label="Locations"
        tools:layout="@layout/fragment_locations" />
    <fragment
        android:id="@+id/optionsFragment"
        android:name="com.example.androidcoursework.fragments.OptionsFragment"
        android:label="Options"
        tools:layout="@layout/fragment_options" />
    <fragment
        android:id="@+id/quotesFragment"
        android:name="com.example.androidcoursework.fragments.quote.QuotesFragment"
        android:label="Quotes"
        tools:layout="@layout/fragment_quotes" />
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.example.androidcoursework.SearchFragment"
        android:label="Search"
        tools:layout="@layout/fragment_search" />
</navigation>